  <Config Name="Q2_HOSTNAME" Target="Q2_HOSTNAME" Default="" Mode="" Description="Quake 2 Hostname: Q2_HOSTNAME" Type="Variable" Display="always" Required="false" Mask="false">My Q2 Server</Config>
  <Config Name="Q2_MAXCLIENTS" Target="Q2_MAXCLIENTS" Default="" Mode="" Description="Maximun players: Q2_MAXCLIENTS" Type="Variable" Display="always" Required="false" Mask="false">10</Config>
  <Config Name="Q2_ADMIN" Target="Q2_ADMIN" Default="" Mode="" Description="Administrator username: Q2_ADMIN" Type="Variable" Display="always" Required="false" Mask="false">admin</Config>
  <Config Name="Q2_ADMIN_PASSWORD" Target="Q2_ADMIN_PASSWORD" Default="" Mode="" Description="Administrator password: Q2_ADMIN_PASSWORD" Type="Variable" Display="always" Required="false" Mask="false">AdminPassw0rd</Config>
