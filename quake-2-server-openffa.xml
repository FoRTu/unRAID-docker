    <Variable>
      <Value>My Q2 Server</Value>
      <Name>Q2_HOSTNAME</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>10</Value>
      <Name>Q2_MAXCLIENTS</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>admin</Value>
